{
  "name": "cnpy-superapp-frontend",
  "version": "0.11.0",
  "private": true,
  "scripts": {
    "preinstall": "npm run security:check-install || true",
    "postinstall": "npm run security:audit || true",
    "prebuild": "npm run security:check-build",
    "build": "next build",
    "postbuild": "npm run security:audit || true",
    "dev": "cross-env NODE_OPTIONS='--no-deprecation' next dev -H 0.0.0.0",
    "dev:turbo": "cross-env NODE_OPTIONS='--no-deprecation' next dev --turbopack -H 0.0.0.0",
    "lint": "eslint .",
    "start": "next start",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:manual": "SKIP_WEBSERVER=1 playwright test",
    "test:e2e:verbose": "playwright test --reporter=list --reporter=html",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "security:check": "npm audit --audit-level=moderate",
    "security:check-install": "npm audit --audit-level=high --production || echo '⚠️  High or critical vulnerabilities detected. Run npm run security:fix to attempt fixes.'",
    "security:check-build": "npm audit --audit-level=high --production || (echo '❌ Build blocked: Critical or high severity vulnerabilities detected!' && echo 'Run: npm run security:audit to see details' && echo 'Run: npm run security:fix to attempt automatic fixes' && exit 1)"
  },
